function [v,s,u_one,J_one] = showOneOptimalWay(u,J)
  len = length(J);
  s = zeros(len,1);
  v = zeros(len,1);
  u_one = zeros(len,1);
  J_one = zeros(len,1);
  
  J_one(1) = J{1}{1}{1};
  tmp = u{1}{1}{1};
  u_one(1) = tmp(1);
  for i =2:len

    J_one(i) = min_mat(1); 
    u_one(i) = find(mat==J_one(i)); 
     
  end
  
end

